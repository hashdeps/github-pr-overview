{
  "name": "@blocks/github-pr-overview",
  "version": "0.1.0",
  "private": true,
  "description": "Display an overview of a GitHub Pull Request, including a timeline of key events",
  "keywords": [
    "blockprotocol",
    "blocks",
    "github",
    "pullrequest"
  ],
  "license": "MIT",
  "author": "Alfred Mountfield",
  "scripts": {
    "build": "block-scripts build",
    "dev": "block-scripts dev",
    "fix:prettier": "prettier --write  --ignore-unknown .",
    "fix:yarn-dedupe": "yarn dedupe",
    "lint:prettier": "prettier --check --ignore-unknown .",
    "lint:tsc": "tsc --noEmit",
    "lint:yarn-dedupe": "yarn dedupe --check",
    "serve": "block-scripts serve"
  },
  "dependencies": {
    "@blockprotocol/graph": "0.0.18",
    "@blockprotocol/type-system": "0.0.4",
    "@hashintel/design-system": "0.0.1",
    "@mui/icons-material": "5.10.6",
    "@mui/lab": "5.0.0-alpha.100",
    "@mui/material": "5.10.6",
    "date-fns": "2.28.0",
    "lodash": "4.17.21",
    "react-transition-group": "4.4.2"
  },
  "devDependencies": {
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "block-scripts": "0.0.14",
    "mock-block-dock": "0.0.38",
    "prettier": "2.8.4",
    "prettier-plugin-packagejson": "2.4.2",
    "prettier-plugin-sh": "0.12.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "packageManager": "yarn@4.0.0-rc.39",
  "block-scripts": {
    "devPort": 9090,
    "servePort": 63222
  },
  "blockprotocol": {
    "blockType": {
      "entryPoint": "react"
    },
    "name": "@alfie/github-pr-overview",
    "displayName": "GitHub Pull-Request Overview",
    "examples": [
      {
        "selectedPullRequest": {
          "repository": "blockprotocol/blockprotocol",
          "number": 298
        }
      }
    ],
    "icon": "public/pull-request.svg",
    "image": "public/block-preview.svg",
    "protocol": "0.2"
  }
}
